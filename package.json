{
  "name": "angular2-full-stack",
  "version": "5.2.0",
  "license": "MIT",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
  "angular-cli": {},
  "engines": {
    "node": "9.8.0",
    "npm": "5.8.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "lint": "npm run lint:style && npm run lint:front && npm run lint:html && npm run lint:back",
    "lint:style": "stylelint 'client/**/**/*{.css,.scss}'",
    "lint:front": "ng lint",
    "lint:back": "tslint --project tsconfig.json server/**/**.ts{,x}",
    "lint:html": "htmllint client/**/**/*.html",
    "lint:autofix": "ng lint --fix && tslint --project tsconfig.json server/**/**.ts{,x} --fix && stylelint 'client/**/*{.css,.scss}' --fix",
    "test:front": "ng test --watch=false",
    "test:back": "tsc -p server && mocha dist/server/test --exit",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@auth0/angular-jwt": "~1.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.13",
    "morgan": "^1.9.0",
    "npm": "^5.8.0",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.6",
    "tether": "1.4.4",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/express": "^4.11.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.0.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.3",
    "protractor": "~5.3.1",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~6.1.0",
    "tslint": "~5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "~2.8.1"
  }
}
