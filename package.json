  "description": "Angular 2 full stack project.",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "prestart": "tsc -p server",
    "start": "concurrently \"mongod\" \"npm run _client:run:w\" \"npm run _server:build:w\" \"npm run _server:run\" ",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod\" \"node server/app.js\"",
    "_client:run:w": "ng serve --proxy-config proxy.conf.json",
    "_client:build": "ng build",
    "_client:build:w": "ng build -w",
    "_server:build": "tsc -p server",
    "_server:build:w": "tsc -w -p server",
    "_server:run": "nodemon",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@types/body-parser": "^1.16.0",
    "@types/express": "^4.0.35",
    "@types/mongoose": "^4.7.8",
    "@types/morgan": "^1.7.32",
    "body-parser": "1.16.1",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "express": "4.14.1",
    "font-awesome": "4.7.0",
    "jquery": "3.1.1",
    "mongoose": "4.9.0",
    "morgan": "1.8.1",
    "rxjs": "^5.1.0",
    "tether": "1.4.0",
    "watch": "^1.0.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.2",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.3.0",
    "jasmine-core": "~2.5.2",
